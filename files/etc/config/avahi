config avahi 'main'
	option enabled '1'
	option reflector '0'
	option enable_dbus '1'
	option host_name 'rk3128-printer'
	option domain_name 'local'
	option browse_domains 'local'
	option use_ipv4 '1'
	option use_ipv6 '0'
	option allow_interfaces 'br-lan,eth0,wlan0'
	option deny_interfaces 'lo'
	option check_response_ttl '1'
	option publish_domain '1'
	option publish_address '1'
	option publish_workstation '1'
	option publish_hinfo '1'
	option publish_aaaa_on_ipv4 '1'
	option publish_a_on_ipv6 '0'

config service
	option name 'CUPS Printer Sharing'
	option type '_ipp._tcp'
	option port '631'
	option txt 'rp=printers/'

config service
	option name 'CUPS Printer'
	option type '_printer._tcp'
	option port '631'
	option txt 'txtvers=1' 'qtotal=1' 'rp=admin/' 'ty=RK3128 Printer Server' 'product=(GPL Ghostscript)' 'pdl=application/postscript,application/vnd.cups-postscript,application/vnd.cups-pdf,application/octet-stream' 'URF=none'
